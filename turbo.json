{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "watch": {
      "cache": false,
      "persistent": false
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "type-check": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
