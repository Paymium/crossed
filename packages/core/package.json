    "@swc/jest": "0.2.21",
    "@types/jest": "28.1.1",
    "@types/node": "18.11.18",
    "@types/react": "^18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-native": "^0.72.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "jest": "^29.5.0",
    "moti": "^0.26.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.3",
    "react-native-reanimated": "^3.4.2",
    "react-native-web": "^0.19.7",
    "ts-node": "10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "*",
    "typescript": "5.1.6"
