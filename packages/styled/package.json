{
  "name": "@crossed/styled",
  "description": "A universal & performant styling library for React Native, Next.js & React",
  "version": "0.13.0",
  "main": "lib/commonjs/index",
  "types": "lib/typescript/index.d.ts",
  "module": "lib/module/index",
  "react-native": "src/index",
  "source": "src/index",
  "typings": "lib/typescript/index.d.ts",
  "keywords": [
    "React Native",
    "Next.js",
    "Expo",
    "React",
    "packager",
    "styling",
    "css-in-js",
    "typescript",
    "javascript",
    "components",
    "android",
    "ios",
    "theme",
    "responsive-styling",
    "universal",
    "ui",
    "theming",
    "generator"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "crossed-build --watch --project-path ./tsconfig.build.json",
    "build": "crossed-build  --project-path ./tsconfig.build.json"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    ".": {
      "require": "./lib/commonjs/index.js",
      "import": "./lib/module/index.js",
      "default": "./lib/module/index.js",
      "types": "./lib/typescript/index.d.ts"
    },
    "./hashCode": {
      "require": "./lib/commonjs/hashCode.js",
      "import": "./lib/module/hashCode.js",
      "default": "./lib/module/hashCode.js",
      "types": "./lib/typescript/hashCode.d.ts"
    },
    "./mq": {
      "require": "./lib/commonjs/mq.js",
      "import": "./lib/module/mq.js",
      "default": "./lib/module/mq.js",
      "types": "./lib/typescript/mq.d.ts"
    },
    "./registry": {
      "require": "./lib/commonjs/Registry.js",
      "import": "./lib/module/Registry.js",
      "default": "./lib/module/Registry.js",
      "types": "./lib/typescript/Registry.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "lib/typescript/index.d.ts"
      ],
      "hashCode": [
        "lib/typescript/hashCode.d.ts"
      ],
      "mq": [
        "lib/typescript/mq.d.ts"
      ],
      "registry": [
        "lib/typescript/Registry.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@crossed/build": "^0.5.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-native": "^0.73.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.73.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.73.2"
  },
  "dependencies": {
    "deepmerge": "^4.3.1"
  }
}
